#include<stdio.h>
struct employee
{
  char name[10];
  int id;
  float salary;
};

int main() {
  struct employee *ptr;
  ptr=(struct employee *)malloc(sizeof(struct employee));
  strcpy(ptr->name,"hema");
 ptr->id=45;
 ptr->salary=4.5;
 printf("%s %d %f",ptr->name,ptr->id,ptr->salary);
 
  return 0;
}
